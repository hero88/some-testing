  "scripts": {
    "set-build-version": "REACT_BUILD_VERSION=$(date +%Y-%m-%d)-$(date +%T) && sed -i -- 's/%REACT_BUILD_VERSION%/'$REACT_BUILD_VERSION'/g' build/index.html && echo React Build Version = $REACT_BUILD_VERSION",
    "start": "env-cmd -f .env.development react-app-rewired start",
    "start:dev": "env-cmd -f .env.development react-app-rewired start",
    "start:staging": "env-cmd -f .env.staging react-app-rewired start",
    "build": "react-app-rewired build && yarn set-build-version",
    "build:dev": "env-cmd -f .env.development react-app-rewired build && yarn set-build-version",
    "build:staging": "env-cmd -f .env.staging react-app-rewired build && yarn set-build-version",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "serve": "serve -s build -l 4000",
    "deploy:version": "yarn install && yarn build && gcloud app deploy --appyaml app.yaml --quiet --version dev-rsm",
    "deploy:uat": "yarn install && yarn build && gcloud app deploy --appyaml app_uat.yaml --quiet",
    "deploy:dev": "yarn install && yarn build:dev && gcloud app deploy --appyaml app_dev.yaml --quiet"
  },
